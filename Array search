class Solution {
static int search(int arr[], int x) {
          int flag=-1;
          for(int i=0;i<arr.length;i++)
          {
              if(arr[i]==x)
              {
              flag =i;
              break;
              }
          }
          if(flag==-1)
          {
             return -1;
          }
          return flag;
          
        
    }
}
