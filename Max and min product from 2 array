class Solution {
    public long findMultiplication(int[] arr1, int[] arr2) {
        int max=arr1[0];
        for(int i=0;i<arr1.length;i++)
        {
            if(arr1[i]>max)
            {
                max=arr1[i];
            }
        }
        int min=arr2[0];
        for(int i=0;i<arr2.length;i++)
        {
            if(arr2[i]<min)
            {
                min=arr2[i];
            }
        }
        return max*min;
    }
}
